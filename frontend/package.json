{
  "name": "cryage-crypto-agent",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "dev:webpack": "next dev",
    "dev:fast": "next dev --turbopack --experimental-debug",
    "dev:turbo": "TURBOPACK=1 next dev --turbopack --experimental-debug --no-open",
    "dev:ultra": "NODE_ENV=development TURBOPACK=1 next dev --port 3000",
    "build": "next build",
    "build:analyze": "ANALYZE=true next build",
    "build:fast": "next build --no-lint",
    "build:prod": "NODE_ENV=production next build",
    "start": "next start",
    "start:prod": "NODE_ENV=production next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "typecheck": "tsc --noEmit",
    "analyze": "node scripts/analyze-bundle.js",
    "perf": "node scripts/performance-check.js",
    "perf:build": "npm run build && npm run analyze",
    "dev:debug": "next dev --turbopack --experimental-debug",
    "build:trace": "next build --debug",
    "size-check": "bundlesize",
    "clean": "rm -rf .next out node_modules/.cache",
    "clean:build": "rm -rf .next out"
  },
  "dependencies": {
    "@ai-sdk/google": "^2.0.11",
    "@ai-sdk/openai": "^2.0.10",
    "@ai-sdk/react": "^2.0.41",
    "@assistant-ui/react": "^0.11.6",
    "@assistant-ui/react-ai-sdk": "^0.11.3",
    "@assistant-ui/react-markdown": "^0.10.8",
    "@emotion/is-prop-valid": "^1.4.0",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-tooltip": "^1.2.7",
    "ai": "^5.0.10",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "framer-motion": "^12.23.12",
    "lucide-react": "^0.539.0",
    "next": "15.4.6",
    "ogl": "^1.0.11",
    "ollama-ai-provider-v2": "^1.2.1",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "remark-gfm": "^4.0.1",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@next/bundle-analyzer": "^15.5.3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^24",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.4.6",
    "tailwindcss": "^4",
    "typescript": "^5",
    "concurrently": "9.2.1"
  }
}
